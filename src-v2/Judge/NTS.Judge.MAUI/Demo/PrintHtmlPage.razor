@using Not.Blazor.Navigation
@using Not.Blazor.PrintPoC
@using static NTS.Judge.Blazor.Pages.Endpoints;

@attribute [Route(PRINT_PAGE)]

<HtmlPrinter Css="@_css" />

@code {
    private string? _css;

    protected override async Task OnInitializedAsync()
    {
        _css = await File.ReadAllTextAsync("./wwwroot/_content/MudBlazor/MudBlazor.min.css");
    }
}
