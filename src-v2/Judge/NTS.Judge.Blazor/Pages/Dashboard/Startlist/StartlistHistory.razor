@using NTS.Domain.Core.Objects
@using NTS.Domain.Objects
@using NTS.Domain.Setup.Entities
@using NTS.Judge.Blazor.Ports
@using System.Collections
@using Not.Blazor.Components
@using Not.Blazor.TM.Models
@using Not.Extensions

@inherits StartlistTable

<NotTrueCenterText Typo="Typo.h5" Content="History" Class="mb-4" />

<MudTabs @ref="@_tabs" PanelClass="px-4 py-6" Elevation="4" Rounded ApplyEffectsToContainer>
    @foreach (var tabHeader in _tabHeaders)
    {
        <MudTabPanel Text="@tabHeader">
            <ChildContent>
                <StartlistTable Headings="_tableHeadings" Items="StartlistByStage[tabHeader]" />
            </ChildContent>
        </MudTabPanel>
    }
</MudTabs>

@code{
    protected string[] _tableHeadings = { "Number", "Athlete", "Loop", "Start Time" };
}