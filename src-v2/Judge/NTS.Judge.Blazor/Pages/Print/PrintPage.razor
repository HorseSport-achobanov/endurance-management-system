@using Not.Blazor.Navigation
@using Not.Blazor.Print

@attribute [Route(PRINT_PAGE)]

<BlazorPrinter Configuration="_navigator.ConsumeParameter<PrintOptions>()" ServiceProvider="_serviceProvider" />

@code {
    [Inject]
    IServiceProvider _serviceProvider { get; set; } = default!;
    [Inject]
    INavigator _navigator { get; set; } = default!;
}
