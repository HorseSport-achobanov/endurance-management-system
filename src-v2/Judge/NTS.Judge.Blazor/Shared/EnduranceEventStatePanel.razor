@using NTS.Domain.Objects
@using NTS.Judge.Blazor.Ports;
@using Not.Blazor.TM
@using Not.Notifier

@inherits NotComponent

<MudStack Class="ma-6">
    <MudButton Color="Color.Primary" Variant="Variant.Filled" OnClick="Start">@Localizer.Get("Start Endurance Event")</MudButton>
</MudStack>

@code {
    [Inject]
    private IDashboardBehind _dashboardBehind { get; set; } = default!;

    public void Start()
    {
        _dashboardBehind.Start();
    }
}