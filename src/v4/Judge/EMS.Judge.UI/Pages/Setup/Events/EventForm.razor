@using Common.Domain;
@using EMS.Domain.Objects;
@using EMS.Judge.Setup.Events;
@using EMS.Judge.UI.Contexts;
@inject ICountriesContext _countries;

<MudCardContent>
    <MudTextField Label="Place" @bind-Value="Model.Place" />
</MudCardContent>

<MudCardContent>
    <MudAutocomplete Label="Country"
        T="Country"
        CoerceText="true"
        @bind-Value="Model.Country"
        SearchFunc="@_countries.Search" />
</MudCardContent>

@code {
    [Parameter]
    public EventUpdateModel Model { get; set; } = null!;
}